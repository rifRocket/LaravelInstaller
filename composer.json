    {
    "name": "rifrocket/laravelinstaller",
    "description": "Laravel web Installation wizard",
    "keywords": [
        "rifrocket",
        "laravelinstaller"
    ],
    "homepage": "https://github.com/rifrocket/laravelinstaller",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mohammad Arif",
            "email": "mohammad.arif9999@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "ext-http": "*"
    },
    "autoload": {
        "psr-4": {
            "RifRocket\\LaravelInstaller\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RifRocket\\LaravelInstaller\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RifRocket\\LaravelInstaller\\LaravelInstallerServiceProvider"
            ],
            "aliases": {
                "LaravelInstaller": "RifRocket\\LaravelInstaller\\LaravelInstallerFacade"
            }
        }
    }
}
